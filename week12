// [w12.01]

#include <iostream>
using namespace std;

int main() {
	int arr[5] = { 11,12,13,14,15 };
	int* ptr = arr;
	cout << "[Print value with array] : ";
	for (int i = 0; i < 5; i++) {
		cout << arr[i] << " ";
	}
	cout << endl;
	cout << "[Print value with pointer] : ";
	for (int i = 0; i < 5; i++) {
		cout << *(ptr + i) << " ";
	}
	cout << endl;

}
